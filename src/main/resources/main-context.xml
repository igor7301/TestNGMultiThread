<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">


       <context:annotation-config/>
       <context:component-scan base-package="com"/>


       <bean class="com.model.search.SearchTestModel" id="searchTestModel">
              <property name="a" value="default"></property>
       </bean>
       <bean class="com.model.search.SearchRamblerModel" id="searchRamblerModel"></bean>
       <bean class="com.model.search.SearchYandexModel" id="searchYandexModel"></bean>
       <bean class="com.model.search.SearchGoogleModel" id="searchGoogleModel"></bean>

       <bean class="com.data.Data" id="data">
              <property name="elemForSearch" value="hello world"></property>
       </bean>

       <bean class="com.model.setup.SetupTemplateModel" id="setupTemplateModel"/>


</beans>